{
  "name": "react-news",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rm -rf public",
    "test": "./node_modules/.bin/karma start",
    "test:watch": "npm test -- --single-run false",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:precommit": "eslint",
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "start": "NODE_ENV=production node ./server/app.js",
    "start:dev": "NODE_ENV=development nodemon ./server/app.js",
    "daemon": "./scripts/daemon.sh",
    "build:dev": "npm run clean && OUTPUT=./public ./node_modules/.bin/webpack --progress --config webpack.config.dev.js",
    "build:prod": "npm run clean && OUTPUT=./public ./node_modules/.bin/webpack --progress --config webpack.config.prod.js",
    "precommit": "./scripts/pre-commit.sh",
    "prepush": "npm run test"
  },
  "dependencies": {
    "body-parser": "1.15.1",
    "bootstrap": "3.3.6",
    "express": "4.13.4",
    "immutable": "3.8.1",
    "keymirror": "0.1.1",
    "mongoose": "4.4.20",
    "morgan": "1.7.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "2.12.0",
    "eslint-plugin-react": "5.1.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.20.0",
    "husky": "0.11.4",
    "isparta-loader": "2.0.0",
    "karma": "0.13.22",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chai-immutable": "1.0.2",
    "karma-coverage": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-jsdom-launcher": "1.0.0",
    "karma-sinon-chai": "1.2.0",
    "karma-webpack": "1.7.0",
    "lolex": "1.5.0",
    "mocha": "2.5.3",
    "nodemon": "1.9.2",
    "react-addons-test-utils": "15.1.0",
    "react-hot-loader": "1.3.0",
    "redux-logger": "2.6.1",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0"
  }
